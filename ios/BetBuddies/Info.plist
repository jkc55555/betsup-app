<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>en</string>
	<key>CFBundleDisplayName</key>
	<string>BetsUp!</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(MARKETING_VERSION)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(CURRENT_PROJECT_VERSION)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSExceptionDomains</key>
		<dict>
			<key>localhost</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<true/>
			</dict>
		</dict>
	</dict>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>BetsUp! uses your location to find nearby friends and events for betting.</string>
	<key>NSCameraUsageDescription</key>
	<string>BetsUp! needs access to your camera to take photos for bet evidence and profile pictures.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>BetsUp! needs access to your photo library to upload bet evidence and profile pictures.</string>
	<key>NSContactsUsageDescription</key>
	<string>BetsUp! uses your contacts to help you find friends to bet with.</string>
	<key>NSFaceIDUsageDescription</key>
	<string>BetsUp! uses Face ID for secure authentication and payment verification.</string>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>armv7</string>
	</array>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UIViewControllerBasedStatusBarAppearance</key>
	<false/>
	<key>UIStatusBarStyle</key>
	<string>UIStatusBarStyleLightContent</string>
	
	<!-- URL Schemes for OAuth and Deep Links -->
	<key>CFBundleURLTypes</key>
	<array>
		<!-- Facebook URL Scheme -->
		<dict>
			<key>CFBundleURLName</key>
			<string>facebook</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>fb$(FACEBOOK_APP_ID)</string>
			</array>
		</dict>
		<!-- Google Sign-In URL Scheme -->
		<dict>
			<key>CFBundleURLName</key>
			<string>google</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>$(REVERSED_CLIENT_ID)</string>
			</array>
		</dict>
		<!-- Venmo Deep Link -->
		<dict>
			<key>CFBundleURLName</key>
			<string>venmo</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>betsup</string>
			</array>
		</dict>
		<!-- Universal Links -->
		<dict>
			<key>CFBundleURLName</key>
			<string>universal</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>https</string>
			</array>
		</dict>
	</array>
	
	<!-- Facebook Configuration -->
	<key>FacebookAppID</key>
	<string>$(FACEBOOK_APP_ID)</string>
	<key>FacebookClientToken</key>
	<string>$(FACEBOOK_CLIENT_TOKEN)</string>
	<key>FacebookDisplayName</key>
	<string>BetsUp!</string>
	
	<!-- LSApplicationQueriesSchemes for external apps -->
	<key>LSApplicationQueriesSchemes</key>
	<array>
		<string>fbapi</string>
		<string>fb-messenger-share-api</string>
		<string>fbauth2</string>
		<string>fbshareextension</string>
		<string>venmo</string>
		<string>paypal</string>
		<string>cashapp</string>
		<string>googlegmail</string>
		<string>comgooglemaps</string>
	</array>
	
	<!-- Associated Domains for Universal Links -->
	<key>com.apple.developer.associated-domains</key>
	<array>
		<string>applinks:betsup.com</string>
		<string>applinks:www.betsup.com</string>
	</array>
	
	<!-- Background Modes -->
	<key>UIBackgroundModes</key>
	<array>
		<string>background-fetch</string>
		<string>remote-notification</string>
		<string>background-processing</string>
	</array>
	
	<!-- App Store Configuration -->
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
	
	<!-- Minimum iOS Version -->
	<key>MinimumOSVersion</key>
	<string>12.4</string>
	
	<!-- App Category -->
	<key>LSApplicationCategoryType</key>
	<string>public.app-category.social-networking</string>
	
	<!-- Appearance -->
	<key>UIUserInterfaceStyle</key>
	<string>Automatic</string>
	
	<!-- Capabilities -->
	<key>UIRequiresFullScreen</key>
	<false/>
	
	<!-- File Sharing -->
	<key>UIFileSharingEnabled</key>
	<false/>
	
	<!-- Scene Configuration for iOS 13+ -->
	<key>UISceneDelegate</key>
	<dict>
		<key>UISceneClassName</key>
		<string>UIWindowScene</string>
		<key>UISceneDelegateClassName</key>
		<string>SceneDelegate</string>
	</dict>
</dict>
</plist>
