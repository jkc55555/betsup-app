# BetsUp! Environment Variables Template
# Copy this file to .env.development, .env.staging, or .env.production
# and fill in your actual values

# Environment
REACT_APP_ENVIRONMENT=development

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=123456789
REACT_APP_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# OAuth Providers
REACT_APP_FACEBOOK_APP_ID=your_facebook_app_id
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:5001

# Monitoring & Analytics
REACT_APP_SENTRY_DSN=https://your_sentry_dsn@sentry.io/project_id
REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
REACT_APP_MIXPANEL_TOKEN=your_mixpanel_token

# Admin Configuration
REACT_APP_ADMIN_API_BASE_URL=https://admin-api.betsup.app

# Feature Flags
REACT_APP_ENABLE_BETA_FEATURES=false
REACT_APP_ENABLE_DEBUG_MODE=false

# Payment Configuration
REACT_APP_VENMO_APP_ID=your_venmo_app_id
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key

# Deep Linking
REACT_APP_DEEP_LINK_SCHEME=betsup
REACT_APP_UNIVERSAL_LINK_DOMAIN=betsup.app

# CDN Configuration
REACT_APP_CDN_BASE_URL=https://cdn.betsup.app

# Social Media
REACT_APP_TWITTER_HANDLE=@BetsUpApp
REACT_APP_INSTAGRAM_HANDLE=@BetsUpApp

# Support
REACT_APP_SUPPORT_EMAIL=support@betsup.app
REACT_APP_SUPPORT_URL=https://help.betsup.bet
